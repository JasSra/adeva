{
  "Diagnostics": {
    "UseDeveloperExceptionPage": false,
    "EfCore": {
      "SensitiveDataLogging": false,
      "DetailedErrors": false
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore": "Information",
        "System": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  }
}
